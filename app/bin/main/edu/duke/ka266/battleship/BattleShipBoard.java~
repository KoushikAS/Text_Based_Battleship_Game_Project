package edu.duke.ka266.battleship;

public class BattleShipBoard {

  private final int width;
  private final int height;

  BattleShipBoard(int w, int h) {
    this.width = w;
    this.height = h;
  }

  public int getWidth() {
    return this.width;
  }

  public int getHeight() {
    return this.height;
  }
}
